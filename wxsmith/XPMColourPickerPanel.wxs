<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="XPMColourPickerPanel">
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<object class="sizeritem">
				<object class="Custom" name="ID_COLOURPICKER1" subclass="XPMColorPicker" variable="ColourPicker" member="yes">
					<creating_code>$(THIS) = new $(CLASS)($(PARENT),$(ID),$(POS),$(SIZE),0,$(NAME));</creating_code>
					<include_file>XPMColorPicker.h</include_file>
					<style></style>
					<tooltip>Colour Picker. Left Click to select a Line Colour. Right Click to select a Fill Colour. Double-click on a colour to redefine it.</tooltip>
					<handler function="OnColourPickerMouseEnter" entry="EVT_ENTER_WINDOW" />
					<handler function="OnColourPickerMouseLeave" entry="EVT_LEAVE_WINDOW" />
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>1</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
